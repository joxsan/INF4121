<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="no.uio.inf5750.assignment2.model.Degree" table="degrees">
    <id name="id" column="id">
      <generator class="native"/>
    </id>
    <property name="type" not-null="true"/>
   
   <set name="courses" table="degree_courses">
   		<key column="degree_id"/>
   		<many-to-many column="course_id"
   			class="no.uio.inf5750.assignment2.model.Course" unique="true"/>
   </set>
   
    <join table="student_degrees" inverse="true">
    	<key column="degree_id"/>
    	<many-to-one column="student_id" name="student"/>
    </join>
    
  </class>
</hibernate-mapping>